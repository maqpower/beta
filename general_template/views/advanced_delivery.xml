<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <template id="template_delivery_advance_layout">
            <!-- Multicompany -->
            <t t-if="o and 'company_id' in o">
                <t t-set="company" t-value="o.company_id"/>
            </t>
            <t t-if="not o or not 'company_id' in o">
                <t t-set="company" t-value="res_company"/>
            </t>
            <t t-call="general_template.delivery_advanced_header"/>
            <div class="article o_report_layout_standard">
                <t t-raw="0"/>
            </div>
            <t t-call="general_template.biz_general_template_custom_footer"/>
        </template>
        <template id="delivery_advanced_header">
            <div class="header">
                <table border="0" cellpadding="0" cellspacing="0" width="100%">
                    <tbody>
                        <tr>
                            <td>
                                <center>
                                    <table border="0" cellpadding="0" cellspacing="0" style="padding:0px 20px 20px 20px; margin:0; font-family: Arial,Helvetica, sans-serif; background:#fff; line-height:20px;" width="100%">
                                        <tr>
                                            <td align="left" colspan="2" style="color:#4D4D4F; font-size:12px; padding:9px 0px 9px 0px;" valign="top">
                                                <table border="0" cellpadding="0" cellspacing="0" width="100%">
                                                    <tr>
                                                        <td align="left" style="color:#4D4D4F; font-size:12px; padding: 30px 0px 0px 0px; margin:0 " valign="middle">
                                                            <table border="0" cellpadding="0" cellspacing="0" width="100%">
                                                                <tr>
                                                                    <td align="left" style="color:#4D4D4F; font-size:12px; padding:0 0px; margin:0 " valign="middle">
                                                                        <img style="max-height: 80px;" t-att-src="'data:image/png;base64,%s' % to_text(o.company_id.invoice_logo) or to_text(o.company_id.logo)" t-if="o.company_id.logo"/>
                                                                    </td>
                                                                </tr>
                                                            </table>
                                                        </td>
                                                        <td align="right" rowspan="3" style="font-size:14px; padding:25px 0px 0px 15px; width:31.5%;" valign="top">
                                                            <h5 t-attf-style="color: #{o.company_id and o.company_id.theme_text_color or '#000000'}; text-align:center;font-weight:bold; background-color: #{o.company_id and o.company_id.theme_color or '#000000'}; text-transform:uppercase; padding:10px 0px; width: 250px;">Delivery Note</h5>
                                                        </td>
                                                    </tr>
                                                </table>
                                            </td>
                                        </tr>
                                    </table>
                                </center>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </template>
        <template id="report_delivery_advanced_main">
            <t t-call="general_template.template_delivery_advance_layout">
                <t t-set="o" t-value="o.with_context({'lang':o.partner_id.lang})"/>
                <div class="page">
                    <div class="oe_structure"/>
                    <table border="0" cellpadding="0" cellspacing="0" width="100%">
                        <tbody>
                            <tr>
                                <td>
                                    <center>
                                        <table border="0" cellpadding="0" cellspacing="0" style="padding:0px 20px 20px 20px; margin:0; font-family: Arial,Helvetica, sans-serif; background:#fff; line-height:20px;" width="100%">
                                            <tr>
                                                <td align="left" colspan="2" t-attf-style="font-size:12px; padding:15px 0px 15px 0px;line-height:18px; border-top:1px solid #{o.company_id and o.company_id.theme_color or '#ececec'};" valign="top">
                                                    <t t-if="o.company_id and o.company_id.is_company_bold">
                                                        <h2 style="text-transform:capitalize; font-size :13px; padding:0px; margin:0;">
                                                            <b>
                                                                <span t-attf-style="color: #{o.company_id and o.company_id.company_color or '#ffffff'};" t-field="o.company_id.partner_id"/>
                                                            </b>
                                                        </h2>
                                                    </t>
                                                    <t t-if="not o.company_id.is_company_bold">
                                                        <h2 style="text-transform:capitalize; font-size :13px; padding:0px; margin:0;">
                                                            <span t-attf-style="color: #{o.company_id and o.company_id.company_color or '#ffffff'};" t-field="o.company_id.partner_id"/>
                                                        </h2>
                                                    </t>
                                                    <div t-attf-style="color: #{o.company_id and o.company_id.company_address_color or '#ffffff'};">
                                                        <span t-esc="o._get_street(o.company_id.partner_id)"/>
                                                        <br/>
                                                        <span t-esc="o._get_address_details(o.company_id.partner_id)"/>
                                                    </div>
                                                    <t t-if="o.company_id.partner_id.mobile">
                                                        <strong t-attf-style="color: #{o.company_id and o.company_id.company_address_color or '#ffffff'};">Mobile: </strong>
                                                        <span t-attf-style="color: #{o.company_id and o.company_id.company_address_color or '#ffffff'};" t-field="o.company_id.partner_id.mobile"/>
                                                        <br/>
                                                    </t>
                                                    <t t-if="o.company_id.partner_id.email">
                                                        <strong t-attf-style="color: #{o.company_id and o.company_id.company_address_color or '#ffffff'};">Email: </strong>
                                                        <span t-attf-style="color: #{o.company_id and o.company_id.company_address_color or '#ffffff'};" t-field="o.company_id.partner_id.email"/>
                                                    </t>
                                                </td>
                                                <td align="right" rowspan="3" style="font-size:14px; padding:15px 0px 15px 15px; width:33%;" valign="top">
                                                    <table border="0" cellpadding="0" cellspacing="0" t-attf-style="color:#fff; font-size:12px; border-collapse: collapse; background-color: #{o.company_id and o.company_id.theme_color or '#000000'}; width: 250px;">
                                                        <tr t-attf-style="color: #{o.company_id and o.company_id.theme_text_color or '#000000'};">
                                                            <td t-attf-style="padding:9px 15px; font-weight:bold; color: #{o.company_id and o.company_id.theme_text_color or '#000000'};">
                                                                <span>Delivery No # </span>
                                                            </td>
                                                            <td t-attf-style="text-align:right; font-weight:bold;padding:9px 15px; color: #{o.company_id and o.company_id.theme_text_color or '#000000'};">
                                                                <span t-field="o.name"/>
                                                            </td>
                                                        </tr>
                                                        <tr t-attf-style="color: #{o.company_id and o.company_id.theme_text_color or '#000000'};">
                                                            <td colspan="2" style="background-color:#f2f2f2; height:4px;"/>
                                                        </tr>
                                                        <tr t-attf-style="color: #{o.company_id and o.company_id.theme_text_color or '#000000'};">
                                                            <td t-attf-style="padding:5px 15px; font-weight:bold; color: #{o.company_id and o.company_id.theme_text_color or '#000000'};">
                                                                <strong>Order (Origin)</strong>
                                                            </td>
                                                            <td t-attf-style="text-align:right; padding:5px 15px; color: #{o.company_id and o.company_id.theme_text_color or '#000000'};">
                                                                <span t-field="o.origin"/>
                                                            </td>
                                                        </tr>
                                                        <tr t-attf-style="color: #{o.company_id and o.company_id.theme_text_color or '#000000'};">
                                                            <td t-attf-style="padding:5px 15px; font-weight:bold; color: #{o.company_id and o.company_id.theme_text_color or '#000000'};">
                                                                <strong>
                                                                    <span class="text-left">Scheduled Date</span>
                                                                </strong>
                                                            </td>
                                                            <td t-attf-style="text-align:right;padding:5px 15px; color: #{o.company_id and o.company_id.theme_text_color or '#000000'};">
                                                                <span t-field="o.scheduled_date"/>
                                                            </td>
                                                        </tr>
                                                        <tr t-attf-style="color: #{o.company_id and o.company_id.theme_text_color or '#000000'};" t-if="o._check_delivery_installed() and o.picking_type_id.code == 'outgoing'">
                                                            <td t-attf-style="padding:5px 15px; font-weight:bold; color: #{o.company_id and o.company_id.theme_text_color or '#000000'};">
                                                                <strong>
                                                                    <span class="text-left">Carrier</span>
                                                                </strong>
                                                            </td>
                                                            <td t-attf-style="text-align:right;padding:5px 15px; color: #{o.company_id and o.company_id.theme_text_color or '#000000'};">
                                                                <span t-field="o.carrier_id"/>
                                                            </td>
                                                        </tr>
                                                        <tr t-attf-style="color: #{o.company_id and o.company_id.theme_text_color or '#000000'};" t-if="o._check_delivery_installed()">
                                                            <td t-attf-style="padding:5px 15px; font-weight:bold; color: #{o.company_id and o.company_id.theme_text_color or '#000000'};">
                                                                <strong>
                                                                    <span class="text-left">Weight</span>
                                                                </strong>
                                                            </td>
                                                            <td t-attf-style="text-align:right;padding:5px 15px; color: #{o.company_id and o.company_id.theme_text_color or '#000000'};">
                                                                <span t-field="o.weight"/>
                                                            </td>
                                                        </tr>
                                                    </table>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td align="left" style="width:50%; padding:15px 0px 15px 0px; line-height:18px;border-top:1px solid #ececec;" valign="top">
                                                    <p t-attf-style="color: #{o.company_id and o.company_id.theme_color or '#000000'}; text-align:left;font-weight:bold;">Delivery To</p>
                                                    <t t-if="o.company_id and o.company_id.is_customer_bold">
                                                        <h1 style="text-transform:capitalize; font-size :13px; padding:0px; margin:0;">
                                                            <b>
                                                                <span t-attf-style="color: #{o.company_id and o.company_id.customer_color or '#ffffff'};" t-field="o.partner_id"/>
                                                            </b>
                                                        </h1>
                                                    </t>
                                                    <t t-if="not o.company_id.is_customer_bold">
                                                        <h1 style="text-transform:capitalize; font-size :13px; padding:0px; margin:0;">
                                                            <span t-attf-style="color: #{o.company_id and o.company_id.customer_color or '#ffffff'};" t-field="o.partner_id"/>
                                                        </h1>
                                                    </t>
                                                    <p t-attf-style="color: #{o.company_id and o.company_id.customer_address_color or '#ffffff'};">
                                                        <span t-esc="o._get_street(o.partner_id)"/>
                                                        <br/>
                                                        <span t-esc="o._get_address_details(o.partner_id)"/>
                                                        <br/>
                                                        <t t-if="o.partner_id.mobile">
                                                            <strong t-attf-style="color: #{o.company_id and o.company_id.customer_address_color or '#ffffff'};">Mobile: </strong>
                                                            <span t-field="o.partner_id.mobile"/>
                                                            <br/>
                                                        </t>
                                                        <t t-if="o.partner_id.email">
                                                            <strong t-attf-style="color: #{o.company_id and o.company_id.customer_address_color or '#ffffff'};">Email: </strong>
                                                            <span t-field="o.partner_id.email"/>
                                                        </t>
                                                    </p>
                                                </td>
                                            </tr>
                                            <tr>
                                                <td t-attf-style="border-bottom:1px solid #{o.company_id and o.company_id.theme_color or '#ececec'}; padding:0px 0px 10px 0px; width:100%;"/>
                                            </tr>
                                            <tr>
                                                <td align="left" colspan="4" style=" font-size:12px; padding:20px 0px 0 0; fnt-weight:bold; width:100%;" valign="top">
                                                    <table cellpadding="0" cellspacing="0" class="table" style="border:0.5px solid #ccc;  border-collapse: collapse; padding:15px; border-bottom:1px solid #ccc;" t-if="not o.move_line_ids" width="100%">
                                                        <thead>
                                                            <tr t-attf-style="page-break-inside: avoid !important;background-color: #{o.company_id and o.company_id.theme_color or '#000000'}; color: #{o.company_id and o.company_id.theme_text_color or '#000000'};">
                                                                <td align="center" t-attf-style="padding:5px 10px; color: #{o.company_id and o.company_id.theme_text_color or '#000000'}; border:1px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; border-right:0px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; font-weight:bold;" width="8%">Sr.</td>
                                                                <td align="left" t-attf-style="padding:5px 10px; color: #{o.company_id and o.company_id.theme_text_color or '#000000'}; border:1px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; border-right:0px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; font-weight:bold;" width="52%">Product</td>
                                                                <td align="right" t-attf-style="padding:5px 10px; color: #{o.company_id and o.company_id.theme_text_color or '#000000'}; border:1px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; border-right:0px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; font-weight:bold;" width="14%">Status</td>
                                                                <td align="right" t-attf-style="padding:5px 10px; color: #{o.company_id and o.company_id.theme_text_color or '#000000'}; border:1px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; border-right:0px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; font-weight:bold;" width="15%">Location</td>
                                                                <td align="right" t-attf-style="padding:5px 10px; color: #{o.company_id and o.company_id.theme_text_color or '#000000'}; border:1px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; border-right:0px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; font-weight:bold;" width="20%">Quantity</td>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <div class="watermark text-center" style="position:absolute;opacity:0.25;font-size:1em;width:80%;z-index:1000;">
                                                                    <img style="max-height: 800px;" t-att-src="'data:image/png;base64,%s' %  to_text(o.company_id.watermark_logo)" t-if="o.company_id"/>
                                                                </div>
                                                            <t t-as="move" t-foreach="o.move_lines">
                                                                <t t-if="move_odd">
                                                                    <tr t-att-style="'page-break-inside: avoid !important;background-color: '+ o.company_id.even_party_color or o.company_id.even_party_color ">
                                                                        <td align="left" class="text-center" t-att-style="'padding:5px 10px; border:0.5px solid #ccc !important; border-right:0px solid #ccc !important; color: %s;' % (o.company_id.text_color)">
                                                                            <span t-esc="move_index+1"/>
                                                                        </td>
                                                                        <td align="left" class="text-left" t-att-style="'padding:5px 10px; border:0.5px solid #ccc !important; border-right:0px solid #ccc !important; color: %s;' % (o.company_id.text_color)">
                                                                            <span t-field="move.product_id and move.product_id.name"/>
                                                                        </td>
                                                                        <td align="center" class="text-right" t-att-style="'padding:5px 10px; border:0.5px solid #ccc !important; border-right:0px solid #ccc !important; color: %s;' % (o.company_id.text_color)">
                                                                            <span t-field="move.state"/>
                                                                        </td>
                                                                        <td align="center" class="text-right" t-att-style="'padding:5px 10px; border:0.5px solid #ccc !important; border-right:0px solid #ccc !important; color: %s;' % (o.company_id.text_color)">
                                                                            <span t-field="move.location_id"/>
                                                                        </td>
                                                                        <td align="right" class="text-right" t-att-style="'padding:5px 10px; border:0.5px solid #ccc !important; border-right:0px solid #ccc !important; color: %s;' % (o.company_id.text_color)">
                                                                            <span t-field="move.product_uom_qty"/>
                                                                            <span t-field="move.product_uom"/>
                                                                        </td>
                                                                    </tr>
                                                                </t>
                                                                <t t-if="move_even">
                                                                    <tr t-att-style="'page-break-inside: avoid !important;background-color: '+ o.company_id.odd_party_color or o.company_id.odd_party_color ">
                                                                        <td align="left" class="text-center" t-att-style="'page-break-inside: avoid !important;padding:5px 10px; border:0.5px solid #ccc !important; border-right:0px solid #ccc !important; color: %s;' % (o.company_id.text_color)">
                                                                            <span t-esc="move_index+1"/>
                                                                        </td>
                                                                        <td align="left" class="text-left" t-att-style="'padding:5px 10px; border:0.5px solid #ccc !important; border-right:0px solid #ccc !important; color: %s;' % (o.company_id.text_color)">
                                                                            <span t-field="move.product_id and move.product_id.name"/>
                                                                        </td>
                                                                        <td align="center" class="text-right" t-att-style="'padding:5px 10px; border:0.5px solid #ccc !important; border-right:0px solid #ccc !important; color: %s;' % (o.company_id.text_color)">
                                                                            <span t-field="move.state"/>
                                                                        </td>
                                                                        <td align="center" class="text-right" t-att-style="'padding:5px 10px; border:0.5px solid #ccc !important; border-right:0px solid #ccc !important; color: %s;' % (o.company_id.text_color)">
                                                                            <span t-field="move.location_id"/>
                                                                        </td>
                                                                        <td align="right" class="text-right" t-att-style="'padding:5px 10px; border:0.5px solid #ccc !important; border-right:0px solid #ccc !important; color: %s;' % (o.company_id.text_color)">
                                                                            <span t-field="move.product_uom_qty"/>
                                                                            <span t-field="move.product_uom"/>
                                                                        </td>
                                                                    </tr>
                                                                </t>
                                                            </t>
                                                        </tbody>
                                                    </table>
                                                    <table cellpadding="0" cellspacing="0" class="table" style="border:0.5px solid #ccc;  border-collapse: collapse; padding:15px; border-bottom:1px solid #ccc;" t-if="o.move_line_ids" width="100%">
                                                        <t groups="stock.group_production_lot" t-set="has_serial_number" t-value="o.move_line_ids.mapped('lot_id')"/>
                                                        <thead>
                                                            <tr t-attf-style="page-break-inside: avoid !important;background-color: #{o.company_id and o.company_id.theme_color or '#000000'}; color: #{o.company_id and o.company_id.theme_text_color or '#000000'};">
                                                                <td align="center" t-attf-style="padding:5px 10px; color: #{o.company_id and o.company_id.theme_text_color or '#000000'}; border:1px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; border-right:0px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; font-weight:bold;" width="8%">Sr.</td>
                                                                <t t-if="o.company_id.add_product_image == True">
                                                                    <td align="left" t-attf-style="padding:5px 10px; color: #{o.company_id and o.company_id.theme_text_color or '#000000'}; border:1px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; border-right:0px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; font-weight:bold;" width="10%">Image</td>
                                                                </t>
                                                                <td align="left" t-attf-style="padding:5px 10px; color: #{o.company_id and o.company_id.theme_text_color or '#000000'}; border:1px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; border-right:0px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; font-weight:bold;" width="52%">Product</td>
                                                                <td align="right" t-attf-style="padding:5px 10px; color: #{o.company_id and o.company_id.theme_text_color or '#000000'}; border:1px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; border-right:0px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; font-weight:bold;" t-if="has_serial_number" width="20%">Serial Number</td>
                                                                <td align="right" t-attf-style="padding:5px 10px; color: #{o.company_id and o.company_id.theme_text_color or '#000000'}; border:1px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; border-right:0px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; font-weight:bold;" width="14%">Status</td>
                                                                <td align="right" t-attf-style="padding:5px 10px; color: #{o.company_id and o.company_id.theme_text_color or '#000000'}; border:1px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; border-right:0px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; font-weight:bold;" width="15%">Location</td>
                                                                <td align="right" t-attf-style="padding:5px 10px; color: #{o.company_id and o.company_id.theme_text_color or '#000000'}; border:1px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; border-right:0px solid #{o.company_id and o.company_id.theme_color or '#000000'} !important; font-weight:bold;" width="20%">Quantity</td>
                                                            </tr>
                                                        </thead>
                                                        <tbody>
                                                            <div class="watermark text-center" style="position:absolute;opacity:0.25;font-size:1em;width:80%;z-index:1000;">
                                                                    <img style="max-height: 800px;" t-att-src="'data:image/png;base64,%s' %  to_text(o.company_id.watermark_logo)" t-if="o.company_id"/>
                                                                </div>
                                                            <t t-as="pack_operation" t-foreach="o.move_line_ids">
                                                                <t t-if="pack_operation_odd">
                                                                    <tr t-att-style="'page-break-inside: avoid !important;background-color: '+ o.company_id.even_party_color or o.company_id.even_party_color ">
                                                                        <td align="left" class="text-center" t-att-style="'padding:5px 10px; border:0.5px solid #ccc !important; border-right:0px solid #ccc !important; color: %s;' % (o.company_id.text_color)">
                                                                            <span t-esc="pack_operation_index+1"/>
                                                                        </td>
                                                                        <t t-if="o.company_id.add_product_image == True">
                                                                            <td class="text-right" t-att-style="'padding:5px 10px; border:0.5px solid #ccc !important; border-right:0px solid #ccc !important; color: %s;' % (o.company_id.text_color)">
                                                                                <span t-field="pack_operation.product_id.image_small" t-options="{&quot;widget&quot;:&quot;image&quot;,&quot;class&quot;:&quot;img-rounded&quot;}"/>
                                                                            </td>
                                                                        </t>
                                                                        <td align="left" class="text-left" t-att-style="'padding:5px 10px; border:0.5px solid #ccc !important; border-right:0px solid #ccc !important; color: %s;' % (o.company_id.text_color)">
                                                                            <span t-field="pack_operation.product_id"/>
                                                                            <t t-if="not pack_operation.product_id and pack_operation.package_id">
                                                                                <span t-field="pack_operation.package_id"/>
                                                                            </t>
                                                                        </td>
                                                                        <t t-if="has_serial_number">
                                                                            <td align="left" class="text-left" t-att-style="'padding:5px 10px; border:0.5px solid #ccc !important; border-right:0px solid #ccc !important; color: %s;' % (o.company_id.text_color)" >
                                                                                <table class="table table-condensed">
                                                                                    <thead>
                                                                                        <tr>
                                                                                            <th>Lot</th>
                                                                                            <th>Qty</th>
                                                                                        </tr>
                                                                                    </thead>
                                                                                    <tr>
                                                                                        <td>
